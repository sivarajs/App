<test-suite relativeUrl="admin" name="T3_CampusTest">
	<test-case name="selectCampusTreeItem">
		<!-- select Setup-->
		<input type="click" xpath=".//*[@id='header']/div[1]/span[1]" />
		
	    <input type="click" xpath=".//*[@id='workArea']/div/div[1]/div[2]/div/div/li[3]/ul/li[2]/div/div[1]" /> 
		<test name="assertTitle">
			<assert entity="Text" operation="Equals" xpath=".//*[@id='contentArea']/div/h1">CAMPUS</assert>
		</test>
	</test-case>
	<test-case name="newCampus">
		<input type="click" xpath=".//*[@id='campusTable']/div[1]/div[1]/div[1]"/>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[2]/div[2]/input">BITSP</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[3]/div[2]/input">Birla Institute Of Technology</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[4]/div[2]/input">BITS Pilani</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[5]/div[2]/select">1</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[6]/div[2]/input">bitspilani.com</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[7]/div[2]/input">01-05-1980</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[8]/div[2]/input">01-05-1980</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[1]/div[9]/div[2]/input">1000</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[2]/div[2]/input">Pilani</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[3]/div[2]/select">1</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[4]/div[2]/select">1</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[5]/div[2]/select">18</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[6]/div[2]/select">1</input>
		<input type="text" xpath=".//*[@id='CampusForm']/div[1]/div[2]/div[7]/div[2]/input">123456</input>
		<input type="click" xpath=".//*[@id='campusSubmit']"/>
	</test-case> 
</test-suite>
